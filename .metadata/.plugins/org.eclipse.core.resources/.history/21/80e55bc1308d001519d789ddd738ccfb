package models;

public class File {
	private int id;
	private int article_id;
	private String path;
	public File(int file_id,int article_id, String path) {
		super();
		this.id = file_id;
		this.article_id = article_id;
		this.path = path;
	}
	public int getId() {
		return id;
	}
	
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	
	
}
