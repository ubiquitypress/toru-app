package models;

public class Section {
	private int id;
	private String title;
	public Section(int section_id, String title) {
		super();
		this.section_id = section_id;
		this.title = title;
	}
	public int getSection_id() {
		return section_id;
	}
	
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	
	
}
